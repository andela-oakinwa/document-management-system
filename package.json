{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "A full-stack document management system",
  "main": "client/Index.jsx",
  "scripts": {
    "start": "node_modules/.bin/babel-node tools/distServer",
    "start:dev": "NODE_ENV=development nodemon --watch server --exec babel-node -- server/Server.js",
    "cover": "NODE_ENV=test node_modules/babel-cli/bin/babel-node.js node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- server/test/**/**/*.js, client/test/**/**/*.js",
    "coverage": "codecov -t 1a7fffda-028f-45ee-a46a-45389d417d58",
    "test": "npm run test:client && test:server",
    "test:client": "NODE_ENV=test mocha --verbose --watch --coverage --reporter progress TestSetup.js client/test/**/*.test.js",
    "test:server": "NODE_ENV=test mocha --compilers js:babel-core/register server/test/**/*.js",
    "test:e2e": "NODE_ENV=test node_modules/.bin/nightwatch --verbose -w --config nightwatch.conf.js",
    "jsdoc": "jsdoc -c conf.json",
    "lint:client": "node_modules/.bin/eslint client/",
    "lint:server": "node_modules/.bin/eslint server/",
    "linting": "npm-run-all lint:server lint:client",
    "drop:db": "psql -c 'drop database if exists doqman_test;' -U postgres",
    "create:db": "psql -c 'create database doqman_test;' -U postgres",
    "migrate:db": "NODE_ENV=test sequelize db:migrate",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R client/test/ && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build:server": "babel server -d build/server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build:prod": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "Oluwafemi Akinwa",
  "repository": {
    "type": "git",
    "url": "https://github.com/andela-oakinwa/document-management-system"
  },
  "license": "MIT",
